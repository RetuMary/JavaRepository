package positivenegative;

public class practice1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		double number=4;
		
		if(number>2) {
			System.out.println("positive");}
		
		else if(number>2) {
			System.out.println("negative");}	
		
		else {
			
			System.out.println("Zero");}
		
	}

}
